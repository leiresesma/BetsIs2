<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://Java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>inicio</managed-bean-name>
		<managed-bean-class>modelo.bean.Inicio</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CreateBean1</managed-bean-name>
		<managed-bean-class>modelo.bean.CreateBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>QueryBean1</managed-bean-name>
		<managed-bean-class>modelo.bean.QueryBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>inicio.xhtml</display-name>
		<from-view-id>/inicio.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/queryQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>queryQuestions.xhtml</display-name>
		<from-view-id>/queryQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>inicio.xhtml</display-name>
		<from-view-id>/inicio.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/createQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>createQuestions.xhtml</display-name>
		<from-view-id>/createQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
